<style>
	.customImg {
		width: 100px;
		height: 100px;
	}

	.paddingT {
		padding-top: 100px;
	}

	.paddingTB {
		padding-bottom: 100px;
		padding-top: 50px;
	}

	.bgWhite {
		background: white;
	}

